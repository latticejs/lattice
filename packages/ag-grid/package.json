{
  "name": "@latticejs/ag-grid",
  "version": "1.0.1-beta.4",
  "license": "MIT",
  "module": "dist/esm/index.js",
  "browser": "dist/umd/ag-grid.js",
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "build:clean": "rimraf dist/**",
    "build": "npm run build:clean && lattice-scripts build ./src/components/index.js",
    "build:dev": "npm run build:clean && lattice-scripts build -e development ./src/components/index.js",
    "release": "npm run build",
    "prepublish": "npm run release",
    "storybook": "start-storybook -p 6006",
    "test": "jest --no-cache",
    "size": "bundlesize",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "ag-grid-community": "^19.1.4",
    "ag-grid-react": "^19.1.2",
    "axios": "^0.20.0",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-external-helpers": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@latticejs/lattice-scripts": "^1.0.1-beta.2",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-controls": "^6.0.27",
    "@storybook/addon-docs": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addon-storyshots": "^6.0.26",
    "@storybook/addon-storysource": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest": "^23.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^2.7.0",
    "storybook-readme": "^5.0.8"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTest.js",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "^.+\\.md?$": "markdown-loader-jest",
      "^.+\\.mdx$": "@storybook/addon-docs/jest-transform-mdx"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/images/",
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  },
  "bundlesize": [
    {
      "path": "dist/cjs/index.js"
    },
    {
      "path": "dist/esm/index.js"
    },
    {
      "path": "dist/umd/gauge.js"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "description": "Standalone Ag Grid Component of Lattice blended with Material UI",
  "main": "dist/cjs/index.js",
  "keywords": [
    "Lattice",
    "Ag-Grid",
    "Ag-Grid Lattice",
    "Lattice Ag-Grid",
    "Grid"
  ],
  "author": ""
}
